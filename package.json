    "url": "https://github.com/CommanderRedYT/realraum-clickhouse-inserter/issues"
  },
  "homepage": "https://github.com/CommanderRedYT/realraum-clickhouse-inserter#readme",
  "devDependencies": {
    "@types/node": "^14.14.31",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.3"
  },
  "dependencies": {
    "axios": "^1.1.3",
    "clickhouse": "^2.6.0",
    "dotenv": "^16.0.3"
